package mypackage;
import java.util.LinkedList;

public class WildAnimal {
       public static void main(String args[]) {
    	   LinkedList<String> list = new LinkedList<String>();
    	   list.add("Cheetah ");
    	   list.add("Tiger ");
    	   list.add("Lion ");
    	   list.add(3,"Elephant ");
    	   list.add(" Anaconda");
    	   System.out.println("LinkedList: " +list);
    	   list.remove("Tiger");
    	   System.out.println("Afte Object Removal: " +list);
    	   list.set(2, "Elephant");
    	   System.out.println("After  Setting An index: " +list);
       }
}
